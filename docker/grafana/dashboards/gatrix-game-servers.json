    {
      "id": 802,
      "title": "Battle Win Rate",
      "type": "gauge",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 96
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(increase(game_battles_completed_total{service=~\"$service\", group=~\"$group\", environment=~\"$environment\", result=\"win\"}[$__range])) / sum(increase(game_battles_completed_total{service=~\"$service\", group=~\"$group\", environment=~\"$environment\"}[$__range])) * 100",
          "legendFormat": "Win Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
