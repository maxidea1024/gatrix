{
  "uid": "gatrix-game-servers",
  "title": "Gatrix Game Servers",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": [
    "gatrix",
    "game",
    "servers"
  ],
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(up{job=\"gatrix-discovered\"}, service)",
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "refresh": 1
      },
      {
        "name": "group",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(up{job=\"gatrix-discovered\"}, group)",
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "refresh": 1
      },
      {
        "name": "environment",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(up{job=\"gatrix-discovered\"}, environment)",
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "refresh": 1
      },
      {
        "name": "window",
        "type": "interval",
        "label": "Window",
        "query": "5m,10m,30m,1h",
        "current": {
          "text": "5m",
          "value": "5m"
        }
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Online Players",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(game_online_players{service=~\"$service\", group=~\"$group\", environment=~\"$environment\"}) by (service)",
          "legendFormat": "{{service}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Connected Clients",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(game_connected_clients{service=~\"$service\", group=~\"$group\", environment=~\"$environment\"}) by (connection_type)",
          "legendFormat": "{{connection_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(game_requests_total{service=~\"$service\", group=~\"$group\", environment=~\"$environment\"}[$window])) by (handler, status)",
          "legendFormat": "{{handler}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Request Duration (p95)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(game_request_duration_seconds_bucket{service=~\"$service\", group=~\"$group\", environment=~\"$environment\"}[$window])) by (handler, le))",
          "legendFormat": "{{handler}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Error Rate (Logic vs HTTP)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(game_errors_total{service=~\"$service\", group=~\"$group\", environment=~\"$environment\"}[$window])) by (type, code)",
          "legendFormat": "Logic: {{type}} - {{code}}"
        },
        {
          "expr": "sum(rate(game_requests_total{service=~\"$service\", group=~\"$group\", environment=~\"$environment\", status=~\"5..\"}[$window])) by (handler, status)",
          "legendFormat": "HTTP: {{handler}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
