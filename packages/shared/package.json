{
  "name": "@gatrix/shared",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "description": "Shared types, constants, and utilities for Gatrix packages",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/errors/index.d.ts",
      "default": "./dist/errors/index.js"
    },
    "./evaluation": {
      "types": "./dist/evaluation/index.d.ts",
      "default": "./dist/evaluation/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "murmurhash": "^2.0.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}