{
  "name": "@gatrix/backend",
  "version": "1.0.0",
  "description": "Gatrix for UWO Backend API - Online Game Platform Management",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "yarn migrate:up && nodemon",
    "dev:no-migrate": "nodemon",
    "build": "tsc",
    "start": "yarn migrate:up && node dist/index.js",
    "start:no-migrate": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:email-template": "node ../../node_modules/ts-node/dist/bin.js src/test/email-template-test.ts",
    "test:email-integration": "node ../../node_modules/ts-node/dist/bin.js src/test/integration-test.ts",
    "test:knex-migration": "node ../../node_modules/ts-node/dist/bin.js src/test/knex-migration-test.ts",
    "check:schema": "node ../../node_modules/ts-node/dist/bin.js src/test/check-schema.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:up": "ts-node src/database/migrate.ts up",
    "migrate:safe": "ts-node src/database/migrate.ts up || echo 'Migration failed but continuing...'",
    "migrate:status": "ts-node src/database/migrate.ts status",
    "migrate:rollback": "ts-node src/database/migrate.ts rollback",
    "db:reset": "ts-node src/database/reset.ts",
    "seed": "ts-node src/database/seed.ts",
    "seed:run": "ts-node src/database/seed.ts run",
    "seed:clear": "ts-node src/database/seed.ts clear",
    "seed:reset": "ts-node src/database/seed.ts reset",
    "sql": "node ../../node_modules/ts-node/dist/bin.js src/scripts/executeSql.ts",
    "migrate:add-usage-count": "node scripts/add-usage-count-column.js",
    "setup:db": "mysql -u root -p < scripts/setup-database.sql",
    "setup:db:win": "scripts\\setup-database.bat",
    "setup:db:unix": "bash scripts/setup-database.sh",
    "mysql:docker": "bash scripts/start-mysql-docker.sh",
    "mysql:docker:win": "scripts\\start-mysql-docker.bat",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
