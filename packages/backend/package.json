    "multer": "^2.0.2",
    "murmurhash": "^2.0.1",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "objection": "^3.1.5",
    "passport": "^0.7.0",
    "passport-baidu": "^0.0.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-qq": "^0.1.0",
    "passport-wechat": "^2.0.4",
    "redis": "^4.6.10",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ssh2": "^1.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ulid": "^3.0.1",
