    "100150000": {
      "id": 100150000,
      "dialogue": [
        "원하는 재료 찾는 중",
        "눈이 너무 아파서 쉬는 중",
        "재료 놓쳐서 발등 찍는 중",
        "찾아낸 재료들을 보며 뿌듯해 하는 중"
      ]
    },
    "100150001": {
      "id": 100150001,
      "dialogue": [
        "타버린 재료 숨기는 중",
        "프라이팬이 무거워 땀 흘리는 중",
        "오븐 가림막 빠르게 닫는 중",
        "한 김 식히는 중"
      ]
    },
    "100150002": {
      "id": 100150002,
      "dialogue": [
        "가루 흘린 것 치우는 중",
        "손에서 반죽 떼는 중",
        "몰래 설탕과 소금을 넣는 중",
        "신나게 반죽을 치대는 중"
      ]
    },
    "100150003": {
      "id": 100150003,
      "dialogue": [
        "재료 크기 자로 재는 중",
        "양손으로 난타 중",
        "종이보다 얇게 자르는 중",
        "여기저기 칼집 넣는 중"
      ]
    },
    "100150004": {
      "id": 100150004,
      "dialogue": [
        "장작에 바람 불어넣는 중",
        "국자로 잘 저어주는 중",
        "큰 거품 터트리는 중",
        "보글보글 소리를 들으며 쉬는 중"
      ]
    },
    "100150005": {
      "id": 100150005,
      "dialogue": [
        "전완근 근력 증가 중",
        "한 방향으로만 젓는 중",
        "매끈하게 다듬는 중",
        "모양새 살피는 중"
      ]
    },
    "100150006": {
      "id": 100150006,
      "dialogue": [
        "너무 큰 덩어리가 걸리는 중",
        "가루가 바깥으로 튀는 중",
        "꼬리 잡기 하는 중",
        "잠시 농땡이 부리는 중"
      ]
    },
    "100150007": {
      "id": 100150007,
      "dialogue": [
        "가마에 장작을 넣는 중",
        "가마 불꽃에 베이컨 굽는 중",
        "조심해서 쇳물을 꺼내는 중",
        "괴를 식히는 중"
      ]
    },
    "100150008": {
      "id": 100150008,
      "dialogue": [
        "설계도 보면서 눈이 침침해 지는 중",
        "톱니바퀴를 틈새에 밀어 넣는 중",
        "조각이 맞는 자리 찾는 중",
        "상자에 장식 새기는 중"
      ]
    },
    "100150009": {
      "id": 100150009,
      "dialogue": [
        "리듬에 맞추어 쇠뭉치 때리는 중",
        "식은 금속을 다시 달구는 중",
        "잘못 내리쳐서 팔 떨리는 중",
        "연마기에서 광택내는 중"
      ]
    },
    "100150010": {
      "id": 100150010,
      "dialogue": [
        "재료 모양내는 중",
        "가마 온도에 놀라는 중",
        "풀무질 하는 중",
        "재료를 조심해서 꺼내는 중"
      ]
    }
